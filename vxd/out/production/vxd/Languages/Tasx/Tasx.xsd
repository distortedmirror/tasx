<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
        targetNamespace="http://www.w3.org/2001/XMLSchema">
    <annotation>
        <documentation>
            Microsoft Visual Studio.NET schema for XSD. This is a reduced version of the
            W3C schema for schemas that has been optimized for use by the Visual Studio.NET
            XML Designer. Information in the official W3C schema for schemas that is not
            used by the Visual Studio.NET XML Designer has been stripped out. For the
            official version of the W3C schema for schemas, please visit http://www.w3.org.
        </documentation>
    </annotation>
    <element name="Language">
        <attribute name="Name">
            <data type="NCName"/>
        </attribute>
        <attribute name="StructureDTD">
            <data type="NCName"/>
        </attribute>
        <element name="Icons">
            <oneOrMore>
                <element name="Icon">
                    <attribute name="Image"/>
                    <attribute name="Name">
                        <data type="NCName"/>
                    </attribute>
                </element>
            </oneOrMore>
        </element>
        <element name="Connectors">
            <oneOrMore>
                <element name="Connector">
                    <attribute name="Image"/>
                    <attribute name="Name">
                        <data type="NCName"/>
                    </attribute>
                    <attribute name="Type">
                        <data type="NCName"/>
                    </attribute>
                </element>
            </oneOrMore>
        </element>
        <element name="Editors">
            <oneOrMore>
                <element name="Editor">
                    <attribute name="AttributeName">
                        <data type="NCName"/>
                    </attribute>
                    <attribute name="Class"/>
                    <attribute name="ElementName">
                        <data type="NCName"/>
                    </attribute>
                    <attribute name="Name">
                        <data type="NCName"/>
                    </attribute>
                    <attribute name="Type">
                        <data type="NCName"/>
                    </attribute>
                </element>
            </oneOrMore>
        </element>
    </element>
</schema>
